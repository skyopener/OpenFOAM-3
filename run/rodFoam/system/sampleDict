FoamFile
{
    version         2.0;
    format          ascii;
    class           dictionary;
    object          sampleDict;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

interpolationScheme cellPoint;

setFormat     raw;

sets      
(
    extract
    {
        type            uniform;
        axis            y;
        start           (0.005 0.0 0.0);
        end             (0.005 0.0999 0.0);
        nPoints         500;  
    }
);

surfaces
();

fields          
(
    
    Ax
    Bz
    
);

